<?xml version="1.0"?>
<web-app>
<display-name>pandemic-duplicate</display-name>
<description>Pandemic Duplicate</description>

<servlet-mapping>
	<url-pattern>/s/deals</url-pattern>
	<servlet-name>PandemicDeal</servlet-name>
</servlet-mapping>

<servlet-mapping>
	<url-pattern>/s/scenarios</url-pattern>
	<servlet-name>Scenario</servlet-name>
</servlet-mapping>

<servlet-mapping>
	<url-pattern>/s/results</url-pattern>
	<servlet-name>PandemicResult</servlet-name>
</servlet-mapping>

<servlet-mapping>
	<url-pattern>/s/games</url-pattern>
	<servlet-name>ActivePlay</servlet-name>
</servlet-mapping>

<servlet-mapping>
	<url-pattern>/s/tournaments</url-pattern>
	<servlet-name>Tournament</servlet-name>
</servlet-mapping>

<servlet-mapping>
	<url-pattern>/s/tournaments/*</url-pattern>
	<servlet-name>Tournament</servlet-name>
</servlet-mapping>

<servlet-mapping>
	<url-pattern>/s/cron/generate_scenario</url-pattern>
	<servlet-name>GenerateScenario</servlet-name>
</servlet-mapping>

<servlet-mapping>
	<url-pattern>/_admin</url-pattern>
	<servlet-name>Admin</servlet-name>
</servlet-mapping>

<servlet>
	<servlet-name>PandemicDeal</servlet-name>
	<servlet-class>pandemic.PandemicDealServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>PandemicResult</servlet-name>
	<servlet-class>pandemic.PandemicResultServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>ActivePlay</servlet-name>
	<servlet-class>pandemic.ActivePlayServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>GenerateScenario</servlet-name>
	<servlet-class>pandemic.GenerateScenarioServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>Scenario</servlet-name>
	<servlet-class>pandemic.ScenarioServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>Tournament</servlet-name>
	<servlet-class>pandemic.TournamentServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>Admin</servlet-name>
	<servlet-class>pandemic.AdminServlet</servlet-class>
</servlet>

</web-app>
